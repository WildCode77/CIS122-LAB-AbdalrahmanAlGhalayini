 @foreach (var p in ViewBag.ListOfSuppliers)
{
    <!--
    Console.WriteLine(p.ToString());
    -->

    <tr>
            <td>@p.SupplierId</td>
            <td>@p.CompanyName</td>
            <td>@p.ContactName</td>
            <td>@p.Address</td>
            <td>@p.City</td>
            <td>@p.Region</td>
            <td>@p.PostalCode</td>
            <td>@p.Country</td>
            <td>@p.Phone</td>
            <td><a href="">@p.HomePage</a></td>
    </tr>

}

 @foreach (var p in ViewBag.ListOfShippers)
{
        <!--
        Console.WriteLine(p.ToString());
